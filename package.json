    "eslint": "8.51.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "mocha": "10.2.0",
    "prettier": "2.8.8",
    "rollup": "4.1.4",
    "sqlite3": "5.1.6",
    "ts-mocha": "10.0.0",
